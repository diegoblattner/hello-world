{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source"
      ]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "dojo-(.+)-(.+)$": "../../packages/\\1/\\2"
        }
      }
    ]
  ]
}
